#!/bin/bash
#файл route_delete который удалял их из таблицы адресации.

#Подсеть 1
#212.100.54.0/26
#mask 255.255.255.192
#ip range 212.100.54.1 - 212.100.54.62
#brodcast 212.100.54.63
#62 host

#Подсеть 2
#212.100.54.64/26
#mask 255.255.255.192
#212.100.54.65 - 212.100.54.126
#brodcast 212.100.54.127
#62 хоста

#Подсеть 3
#212.100.54.128/26
#mask 255.255.255.192
#212.100.54.129 - 212.100.54.190
#brodcast 212.100.54.191
#62 хоста

#Подсеть 4
#212.100.57.192/26
#mask 255.255.255.192
#212.100.57.193 - 212.100.57.254
#brodcast 212.100.54.255
#62 хоста

sudo ip route del 212.100.54.0/26 dev eth0
sudo route del -net 212.100.54.64/26 gw 212.100.54.1 dev eth0
sudo route del -net 212.100.54.128/26 gw 212.100.54.1 dev eth0
sudo route del -net 212.100.57.192/26 gw 212.100.54.1 dev eth0

#Result - https://prnt.sc/2XMXEXhqEMbE
